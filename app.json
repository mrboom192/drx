{
  "expo": {
    "name": "DrX Telehealth",
    "slug": "drx",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "drx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doctorx.drx",
      "bitcode": false,
      "appleTeamId": "46LZA2WXBP",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleLocalizations": [
          "en",
          "ar"
        ]
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.doctorx.drx"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.doctorx.drx",
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-document-picker",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      "expo-font",
      "expo-secure-store",
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "DrX Telehealth requires access to the camera so that the other person may see you during consultations.",
          "microphonePermission": "DrX Telehealth requires access to the microphone so that the other person can hear you during consultations."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.doctorx.drx",
          "publishableKey": "pk_live_51Q6gpGJIvTh9EQNO1CZxko18GxTEMe5FrMqyFBAxN5E3DnruXRBMMBv3vJaJIXxgn7BxkOm79h7aKHZbwtlQafQW00xIvxrDKP",
          "enableGooglePay": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "DrX Telehealth requires access to your location to help you find nearby healthcare providers when using the map."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "81b03d8c-3eed-4659-a28f-7ebbf6d4d6d5"
      },
      "supportsRTL": true
    },
    "owner": "mrboom192"
  }
}
